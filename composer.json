{
    "name": "enesyurtlu/modman",
    "description": "A Module Management for Laravel 6.x",
    "license": "MIT",
    "keywords": [
        "modman"
    ],
    "type": "library",
    "authors": [{
        "name": "Enes Yurtlu",
        "email": "meyurtlu@gmail.com"
    }],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "enesyurtlu\\modman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "enesyurtlu\\modman\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "enesyurtlu\\modman\\ModManServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
